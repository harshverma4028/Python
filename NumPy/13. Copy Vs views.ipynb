   "source": [
    "import numpy as np\n",
    "\n",
    "var=np.array([1,2,3,4,5])\n",
    "\n",
    "co=var.copy()\n",
    "var[1]=32;\n",
    "\n",
    "print(\"the var is: \",var)\n",
    "print(\"the copy is : \",co)"
   ]
